{
  "name": "votingapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.17.1",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.18.2",
    "chart.js": "^2.7.1",
    "classnames": "^2.2.5",
    "concurrently": "3.5.1",
    "jsonwebtoken": "^8.1.1",
    "jwt-decode": "^2.2.0",
    "lodash": "4.17.5",
    "publicip": "^1.0.0",
    "react": "16.2.0",
    "react-chartjs-2": "^2.7.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.0",
    "redux": "3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "2.2.0",
    "validator": "9.4.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "postinstall": "yarn install --prefix backend",
    "start": "concurrently \"cd backend && yarn start\" \"cd ./ && yarn run dev \" ",
    "build:prod": "concurrently \"cd ./ && yarn run build\" \"cd backend && yarn run build\"",
    "start:prod": "cd backend && yarn run start:prod",
    "heroku-postbuild": "yarn run build:prod"
  },
  "proxy": "http://localhost:8080"
}
